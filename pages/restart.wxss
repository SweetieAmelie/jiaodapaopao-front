/* restart.wxss */
.unauthorizedFrame{
  margin-top: 200rpx;
  display: flex;
  width: 750rpx;
  flex-direction: column;
  align-items: center;
}
.unauthorizedFrame text{
  margin: 0 auto;
  width: 650rpx;
  font-size: 32rpx;
  color: #4a4a4a;
  line-height: 36rpx;
}
.buttons{
  font-size: 28rpx;
  margin-top: 40rpx;
  display: flex;
  flex-direction: row;
}
.buttons button{
  background-color: #3db4f4;
  color: #fff;
  font-size: 28rpx;
  margin-right: 20rpx;
}
.buttons view{
  margin: auto 0;
  color: #fe3851
}